            xval[k*ldx+j] = dbeta*xval[k*ldx+j];
        }
    }
#pragma omp parallel private(j,k)
    {
        complex double *x_priv;
        ghost_malloc((void **)&x_priv,CFGM*CFGK*sizeof(complex double));
        memset(x_priv,0,CFGM*CFGK*sizeof(complex double));
        
        if (conjv) {

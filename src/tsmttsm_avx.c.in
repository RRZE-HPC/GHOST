                    }
                    for (; m<ldv; m++) {
                        xval[k*ldx+m] += x_priv[k*ldxpriv+m];
                    }
                }
            }
            free(x_priv);
    }
            
    } else {
        ERROR_LOG("Will be implemented soon :-)!");
        ret = GHOST_ERR_NOT_IMPLEMENTED;
        goto err;
    }
   
    goto out;
err:

out:
    GHOST_FUNC_EXIT(GHOST_FUNCTYPE_MATH)
    return ret;

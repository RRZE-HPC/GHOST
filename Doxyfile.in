PROJECT_NAME = @CMAKE_PROJECT_NAME@
OUTPUT_DIRECTORY = doc/
EXTRACT_STATIC = no
EXTRACT_PRIVATE = no
INPUT = @CMAKE_SOURCE_DIR@ @CMAKE_SOURCE_DIR@/src/ @CMAKE_SOURCE_DIR@/include/ @CMAKE_SOURCE_DIR@/include/ghost/
GENERATE_LATEX = no
GENERATE_HTML = yes
HTML_OUTPUT = html
SHOW_INCLUDE_FILES = yes
HIDE_UNDOC_CLASSES = yes
HAVE_DOT = yes
CALL_GRAPH = yes
EXCLUDE_PATTERNS = *.F90
PREDEFINED = "GHOST_HAVE_LONGIDX=@GHOST_HAVE_LONGIDX@" \
              "GHOST_HAVE_CUDA=@GHOST_HAVE_CUDA@"

